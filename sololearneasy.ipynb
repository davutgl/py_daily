    "dollars = int(input(\"Enter price in dollars: \"))\n",
    "pesos = int(input(\"Enter price in pesos: \"))\n",
    "cents = dollars * 100\n",
    "peso = int((2 * cents)/100)\n",
    "if cents > peso:\n",
    "    print(\"Pesos\")\n",
    "else:\n",
    "    print(\"Dollars\")\n",
